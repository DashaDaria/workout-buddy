<div class="container">

      <ul>
      <% if current_user %>
        <li><a <%= link_to "Hi, #{current_user.name}", user_workouts_path(current_user) %></a></li>
        <li><a <%= link_to "Log Out", logout_path, method: :delete %></a></li>
      <% else %>
        <li><a <%= link_to "Home", root_path %></a></li>
        <li><a <%= link_to "Log In", login_path %></a></li>
        <li><a <%= link_to "Sign Up", signup_path %></a></li>
      <% end %>

      <li><a <%= link_to "Exercises", exercises_path %></a></li>
    </ul>

  </div>
